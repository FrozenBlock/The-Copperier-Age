{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "The Copperier Age",
  "description": "Enhances the visuals and scope of The Copper Age drop.",
  "authors": [
    "Lunade"
  ],
  "contributors": [
    "Lufurrius",
    "zhen",
    "voxelotl"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/the-copperier-age",
    "sources": "https://github.com/FrozenBlock/TheCopperierAge",
